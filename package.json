{
  "name": "chat-gpt-nestjs",
  "version": "0.6.0",
  "description": "Empower nestjs with native OpenAi integration",
  "author": "SanYann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SanYann/chat-gpt-nestjs/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts' --fix",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm test && npm run build && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --passWithNoTests",
    "version": "npm run format && git add -A src",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SanYann/chat-gpt-nestjs"
  },
  "keywords": [
    "chatgpt",
    "openai",
    "nestjs",
    "typescript"
  ],
  "dependencies": {
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.0.0",
    "axios": "^1.4.0",
    "cache-manager": "^5.2.1",
    "openai": "^3.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "homepage": "https://github.com/SanYann/chat-gpt-nestjs#readme",
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  }
}